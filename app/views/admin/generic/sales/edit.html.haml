%h1 Edit Sale
= form_for @sale, :url => [:admin, :generic, @sale] do |f|
  = render :partial => 'form', :locals => {:f => f}
  .actions= f.submit 'Update' , :class => 'button'
%p
  = link_to "Show", [:admin, :generic, @sale], :class => 'button'
  |
  = link_to "View All", admin_generic_sales_path, :class => 'button'
