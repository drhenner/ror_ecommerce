<%# render :partial => '/admin/merchandise/sub_header'%>
<% content_for :header_sub_bar do -%>
  <%= link_to 'Products', admin_merchandise_products_path %> |
  <%= link_to 'Properties', admin_merchandise_properties_path %> |
  <%= link_to 'Product Types', admin_merchandise_product_types_path %> |
  <%= link_to 'Prototypes', admin_merchandise_prototypes_path %> |
  <%= link_to 'Brands', admin_merchandise_brands_path %>
<% end %>


<% if @product && @product.id %>
  <% content_for :sidemenu do -%>

    <ul>
      <!-- <li> <%#= link_to 'Brand',         edit_admin_merchandise_brand_path(@product.brand) %> </li> -->
        <li> <%= link_to 'Description', edit_admin_merchandise_products_description_path(@product) %> </li>
        <li> <%= link_to 'Images',        edit_admin_merchandise_images_product_path(@product) %> </li>

        <li> <%= link_to 'Product', edit_admin_merchandise_product_path(@product) %> </li>
        <li> <%= link_to 'Variants', edit_variants_admin_merchandise_product_path(@product) %> </li>
    </ul>

  <% end %>
<% end %>